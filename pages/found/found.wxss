/* pages/found/found.wxss */
@import "/colorui/icon.wxss";
@import "/colorui/main.wxss";

.img {
  width: 1.5rem;
  height: 1.5rem;
}
.headPhone {
  width: 3rem;
  height: 3rem;
  margin-right: 1rem;
}
.rank {
  height: 5rem;
  margin: 0 0.5rem;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction: row;
}
.number {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width: 10rem;
  height: 1.5rem;
}
.rankindex {width: 1.5rem; text-align: center;}
.number .img, .number .rankindex {margin-right: 1rem;}
.star {
  display: flex;
  justify-content: center;
  column-gap: 0.5rem;
  align-items: center;
}
.foundPage {
  flex-direction: column;
  font-size: 0.8rem;
}

.foundTab {
  display: flex;
  justify-content: space-around;
  width: 100%;
}
